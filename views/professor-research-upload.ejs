          <div class="card p-4 shadow-lg">
            <h2 class="text-center mb-4" style="color: #526ebb;">Uploade file</h2>
            <div class="accordion" id="faqAccordion">
      <form action="/professor-research-upload" method="POST" enctype="multipart/form-data">
              <!-- Q1 -->
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingOne">
                  <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne">
                    General
                  </button>
                </h2>
                <div id="collapseOne" class="accordion-collapse collapse show" data-bs-parent="#faqAccordion">
                  <div class="accordion-body">
                    <table>
                        <tr>
                            <th>Project title : </th>
                            <th>
                                <div class="wave-group" style="margin: 0 0 0 30px;">
            <input required="" type="text" class="input" id="name" name = "title">
            <span class="bar"></span>
            <label class="label">
                <span class="label-char" style="--index: 0">P</span>
                <span class="label-char" style="--index: 1">r</span>
                <span class="label-char" style="--index: 2">o</span>
                <span class="label-char" style="--index: 3">j</span>
                <span class="label-char" style="--index: 4">e</span>
                <span class="label-char" style="--index: 5">c</span>
                <span class="label-char" style="--index: 6">t</span>
                <span class="label-char" style="--index: 7">&nbsp;</span>
                <span class="label-char" style="--index: 8">n</span>
                <span class="label-char" style="--index: 9">a</span>
                <span class="label-char" style="--index: 10">m</span>
                <span class="label-char" style="--index: 11">e</span>
            </label>
        </div>
                            </th>
                        </tr>

                        <tr>
                            <th style="margin: 60px 0 0 0;"><label for="projectYear" class="mb-3">Content :</label></th>
                            <th style="margin: 50px 0 0 60px;">
                                
            <div class="form-group">
              <div class="wave-group">
                  <select id="projectYear" class="input" required name="status">
                      <option value="" disabled selected>&nbsp;</option>
                      <option value="year1">Completed</option>
                      <option value="year2">In progress</option>
                  </select>
                  <span class="mt-4" ></span>
                  <label class="label">
                      <span class="label-char" style="--index: 0">S</span>
                      <span class="label-char" style="--index: 1">e</span>
                      <span class="label-char" style="--index: 2">l</span>
                      <span class="label-char" style="--index: 3">e</span>
                      <span class="label-char" style="--index: 4">c</span>
                      <span class="label-char" style="--index: 5">t</span>
                      <span class="label-char" style="--index: 6">&nbsp;</span>
                      <span class="label-char" style="--index: 7">c</span>
                      <span class="label-char" style="--index: 8">o</span>
                      <span class="label-char" style="--index: 9">n</span>
                      <span class="label-char" style="--index: 10">t</span>
                      <span class="label-char" style="--index: 11">e</span>
                      <span class="label-char" style="--index: 12">n</span>
                      <span class="label-char" style="--index: 13">t</span>
                  </label>
              </div>
            </div>

                            </th>
                        </tr>
                    </table>
                  </div>
                </div>
              </div>
      

              <!-- Q2 -->
              <div class="accordion-item" >
                <h2 class="accordion-header" id="headingTwo" >
                  <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseTwo" >
